                encrypted-password "$6$UfxCExXB$lyKzl4SD0TM2Ygu9bIfG.BTmwQ9S1fCahnAW8DJSizcuDuDnfL0XGMlxRtrtiIt0pxaoPaJCPpEI1MegscoZ3/"
                plaintext-password ""
