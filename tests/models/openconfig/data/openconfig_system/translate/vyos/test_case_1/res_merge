set system static-host-mapping host-name domain-v6.com inet '1234:ab:cd:ef::1'
set system static-host-mapping host-name domain-v4.com alias 'alias4-domain'
set system static-host-mapping host-name domain-v4.com inet '1.2.3.4'
set system ntp server '0.pool.ntp.org'
set system ntp server 0.pool.ntp.org prefer ''
